{
  "name": "starter-basic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "yarn cache clean && rm -rf node_modules yarn.lock .next && yarn install",
    "dev": "concurrently \"yarn watchAssets\" \"tinacms dev -c \\\"next dev\\\"\"",
    "build": "yarn buildAssets && tinacms build && next build",
    "buildNext": "next build",
    "watchAssets": "WATCHING=true yarn buildAssets",
    "buildAssets": "yarn svgs && yarn imgs",
    "imgs": "node utils/imagesOptimizeAndConvert.js",
    "svgs": "node utils/svgsOptimizeAndConvert.js",
    "start": "tinacms build && next start",
    "tinaaudit": "tinacms audit",
    "lint": "next lint"
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tinacms/react-core": "^0.43.0",
    "axios": "^1.7.2",
    "chokidar": "^3.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "es6-promise": "^4.2.8",
    "express": "^4.19.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "gm": "^1.25.0",
    "next": "^14.2.5",
    "promise-polyfill": "^8.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-microsoft-clarity": "^1.2.0",
    "react-svg": "^16.1.34",
    "react-syntax-highlighter": "^15.5.0",
    "react-tinacms-github": "^0.52.0",
    "run-p": "^0.0.0",
    "sass": "~1.32.6",
    "styled-components": "^6.1.11",
    "tinacms": "^1.6.7",
    "typescript": "5.5.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@tinacms/cli": "^1.5.49",
    "@types/node": "^20.14.9",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.4",
    "npm-run-all": "^4.1.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^14.2.1",
    "svgo": "^3.3.2",
    "svgo-loader": "^4.0.0"
  }
}
