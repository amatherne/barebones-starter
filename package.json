{
  "name": "starter-basic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn watchAssets\" \"NODE_NO_WARNINGS=1 tinacms dev -c \\\"NODE_NO_WARNINGS=1 next dev\\\"\"",
    "build": "yarn buildAssets && tinacms build && yarn buildNext",
    "buildTest": "cross-env ANALYZE=true yarn buildNext",
    "buildNext": "next build",
    "watchAssets": "WATCHING=true yarn buildAssets",
    "buildAssets": "yarn svgs && yarn imgs",
    "imgs": "node utils/imagesOptimizeAndConvert.js",
    "svgs": "node utils/svgsOptimizeAndConvert.js",
    "start": "tinacms build && next start",
    "tinaaudit": "tinacms audit",
    "lint": "next lint",
    "reboot": "yarn cache clean && rm -rf node_modules yarn.lock .next tina/__generated__ && yarn install && tinacms build",
    "dev_trace": "concurrently \"yarn watchAssets\" \"node --trace-deprecation node_modules/.bin/tinacms dev -c \\\"next dev\\\"\""
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@svgr/webpack": "^8.1.0",
    "@tinacms/react-core": "^0.43.0",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.7.2",
    "chokidar": "^3.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "dompurify": "^3.1.6",
    "es6-promise": "^4.2.8",
    "express": "^4.19.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "gm": "^1.25.0",
    "googleapis": "^140.0.1",
    "he": "^1.2.0",
    "next": "^14.2.5",
    "nodemailer": "^6.9.14",
    "promise-polyfill": "^8.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-microsoft-clarity": "^1.2.0",
    "react-svg": "^16.1.34",
    "react-syntax-highlighter": "^15.5.0",
    "react-tinacms-github": "^0.52.0",
    "run-p": "^0.0.0",
    "sanitize-html": "^2.13.0",
    "sass": "~1.32.6",
    "styled-components": "^6.1.11",
    "terser-webpack-plugin": "^5.3.10",
    "tinacms": "^1.6.7",
    "typescript": "5.5.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@tinacms/cli": "^1.5.49",
    "@types/node": "^20.14.9",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.4",
    "npm-run-all": "^4.1.5",
    "sass-lint": "^1.13.1",
    "sass-loader": "^14.2.1",
    "svgo": "^3.3.2",
    "svgo-loader": "^4.0.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
